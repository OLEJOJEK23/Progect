import math
formula = int(input("Введите номер задания : "))
match formula:
    case 1:
        result = 0
        print("Путь,время,скорость.\n Основная формула: S = v * t ")
        finding = str(input("Введите искомое: "))
        if finding == "S":
            v = float(input("Введите скорость : "))
            t = float(input("Введите время : "))
            result = v * t
        elif finding == "v":
            S = float(input("Введите путь : "))
            t = float(input("Введите время : "))
            result = S/t
        elif finding == "t":
            S = float(input("Введите путь : "))
            v = float(input("Введите скорость : "))
            result = S/v
        else:
            print("Переменная не найдена.")
        print(f"Ответ:{result}")
    case 2:
        result = 0
        print("Равномерно ускоренное движение.Ускорение.\n Основная формула: a = Δv/t ")
        finding = str(input("Введите искомое: "))
        if finding == "a":
            delv = float(input("Введите изменение скорости, если оно не известно введите 0: "))
            if delv == 0:
                v1 = float(input("Введите начальную скорость: "))
                v2 = float(input("Введите конечную скорость: "))
            t = float(input("Введите время : "))
            result = delv / t
        elif finding == "Δv":
            a = float(input("Введите ускорение : "))
            t = float(input("Введите время : "))
            result = a * t
        elif finding == "t":
            a = float(input("Введите ускорение : "))
            delv = float(input("Введите изменение скорости, если оно не известно введите 0  : "))
            if delv == 0:
                v1 = float(input("Введите начальную скорость: "))
                v2 = float(input("Введите конечную скорость: "))
            result = delv/a
        else:
            print("Переменная не найдена.")
        print(f"Ответ:{result}")
    case 3:
        print("Равномерно ускоренное движение.Путь.\n Основная формула: S = v * t + (a * t^2) /2 ")
        v = float(input("Введите  скорость: "))
        t = float(input("Введите время : "))
        a = float(input("Введите ускорение : "))
        result = v * t + (a * (t ** 2)) / 2
        print(f"Ответ:{result}")
    case 4:
        print("Высота тела брошенного вертикально вверх.\n Основная формула: h = h0 + v0 * t - (g * t^2) /2 ")
        h0 = float(input("Введите изначальную высоту: "))
        v0 = float(input("Введите изначальную скорость: "))
        t = float(input("Введите время: "))
        g = 9.8
        result = h0 + v0 * t - (g * (t**2))/2
        print(f"Ответ:{result}")
    case 5:
        print("Скорость тела брошенного вертикально вверх.\n Основная формула: v =  v0 - g * t  ")
        v0 = float(input("Введите изначальную скорость: "))
        t = float(input("Введите время: "))
        g = 9.8
        result = v0 - g * t
        print(f"Ответ:{result}")
    case 6:
        result = 0
        print("Скорость, ускорение, время.\n Основная формула: v =  a * t  ")
        finding = str(input("Введите искомое: "))
        if finding == "a":
            v = float(input("Введите скорость : "))
            t = float(input("Введите время : "))
            result = v / t
        elif finding == "v":
            a = float(input("Введите ускорение : "))
            t = float(input("Введите время : "))
            result = a * t
        elif finding == "t":
            a = float(input("Введите ускорение : "))
            v = float(input("Введите скорость : "))
            result = v/a
        else:
            print("Переменная не найдена.")
        print(f"Ответ:{result}")
    case 7:
        result = 0
        print("Центростремительное ускорение.\n Основная формула: a = v^2/R  ")
        finding = str(input("Введите искомое: "))
        if finding == "a":
            v = float(input("Введите скорость : "))
            R = float(input("Введите радиус : "))
            result = v**2 / R
        elif finding == "v":
            a = float(input("Введите ускорение : "))
            R = float(input("Введите радиус : "))
            result = math.sqrt(a * R)
        elif finding == "R":
            a = float(input("Введите ускорение : "))
            v = float(input("Введите скорость : "))
            result = v**2 / a
        else:
            print("Переменная не найдена.")
        print(f"Ответ:{result}")
    case 8:
        result = 0
        print("Угловая скорость.\n Основная формула: ω = φ/t  ")
        finding = str(input("Введите искомое: "))
        if finding == "ω":
            fi = float(input("Введите угол : "))
            t = float(input("Введите время : "))
            result = fi/t
        elif finding == "φ":
            omega = float(input("Введите угловую скорость : "))
            t = float(input("Введите время : "))
            result = omega * t
        elif finding == "t":
            fi = float(input("Введите угол : "))
            omega = float(input("Введите угловую скорость : "))
            result = fi / omega
        else:
            print("Переменная не найдена.")
        print(f"Ответ:{result}")
    case 9:
        print("Равномерное круговое движение.\n Основная формула: l = R * φ ")
        R = float(input("Введите радиус: "))
        fi = float(input("Введите угол: "))
        result = R * fi
        print(f"Ответ:{result}")
    case 10:
        result = 0
        print("Равномерное круговое движение: линейная скорость.\n Основная формула: v = R * ω ")
        finding = str(input("Введите искомое: "))
        if finding == "v":
            omega = float(input("Введите угловую скорость : "))
            R = float(input("Введите радиус : "))
            result = R * omega
        elif finding == "ω":
            v = float(input("Введите линейную скорость : "))
            R = float(input("Введите радиус : "))
            result = v/R
        elif finding == "R":
            v = float(input("Введите линейную скорость : "))
            omega = float(input("Введите угловую скорость : "))
            result = v/omega
        else:
            print("Переменная не найдена.")
        print(f"Ответ:{result}")
    case 11:
        print("Период вращения1.\n Основная формула: T = t/N ")
        t = float(input("Введите время: "))
        N = float(input("Введите число вращений: "))
        result = t/N
        print(f"Ответ:{result}")
    case 12:
        print("Период вращения2.\n Основная формула: T = 2*π*R /v  ")
        pi = math.pi
        R = float(input("Введите радиус: "))
        v = float(input("Введите линейную скорость: "))
        result = (2 * pi * R) / v
        print(f"Ответ:{result}")
    case 13:
        print("Период вращения3.\n Основная формула: T = 2*π/ω  ")
        pi = math.pi
        omega = float(input("Введите угловую скорость: "))
        result = (2 * pi) / omega
        print(f"Ответ:{result}")
    case 14:
        print("Центростремительное ускорение.\n Основная формула: a = 4 * π^2 * R * n^2  ")
        pi = math.pi
        R = float(input("Введите радиус: "))
        n = float(input("Введите частоту вращения: "))
        result = 4 * pi**2 * R * n ** 2
        print(f"Ответ:{result}")
    case 15:
        result = 0
        print("Частота вращения.\n Основная формула: n = 1/T ")
        finding = str(input("Введите искомое: "))
        if finding == "n":
            T = float(input("Введите период вращения : "))
            result = 1/T
        elif finding == "T":
            n = float(input("Введите частоту вращения : "))
            result = 1/n
        else:
            print("Переменная не найдена.")
        print(f"Ответ:{result}")
    case 16:
        print("Центростремительное ускорение.\n Основная формула: a = ω^2 * R  ")
        R = float(input("Введите радиус: "))
        omega = float(input("Введите угловую скорость: "))
        result = omega ** 2 * R
        print(f"Ответ:{result}")
    case 17:
        print("Основное уравнение молекулярно-кинетической теории.\n Основная формула: p = 1/3 * n * m0 * v^2  ")
        n = float(input("Введите концентрацию: "))
        m0 = float(input("Введите массу молекулы: "))
        v = float(input("Введите скорость: "))
        result = 1/3 * n * m0 * v**2
        print(f"Ответ:{result}")
    case 18:
        print("Концентрация молекул.\n Основная формула: n = N/V  ")
        N = float(input("Введите число молекул: "))
        V = float(input("Введите объём: "))
        result = N/V
        print(f"Ответ:{result}")
    case 19:
        print("Давление идеального газа.\n Основная формула: p = 2/3 * n * E  ")
        E = float(input("Введите энергию: "))
        n = float(input("Введите концентрацию: "))
        result = 2/3 * n * E
        print(f"Ответ:{result}")
    case 20:
        print("Внутренняя энергия молекул.\n Основная формула: E = m * v^2 /2  ")
        m = float(input("Введите массу: "))
        v = float(input("Введите скорость: "))
        result = m * (v ** 2) / 2
        print(f"Ответ:{result}")
    case 21:
        print("Средняя кинетическая энергия.\n Основная формула: E  = (3 * k * T)/2 ")
        k = 1.36 * 10 ** -23
        T = float(input("Введите температуру: "))
        result = (3 * k * T)/2
        print(f"Ответ:{result}")
    case 22:
        print("Уравнение состояния идеального газа.\n Основная формула: p = vRT/V ")
        R = 8.3
        T = float(input("Введите температуру: "))
        V = float(input("Введите объём: "))
        v = float(input("Введите скорость: "))
        result = (v * R * T) / V
        print(f"Ответ:{result}")
    case 23:
        result = 0
        print("Сила, масса, ускорение.\n Основная формула: F = m * a ")
        finding = str(input("Введите искомое: "))
        if finding == "F":
            m = float(input("Введите массу : "))
            a = float(input("Введите ускорение : "))
            result = m * a
        elif finding == "m":
            F = float(input("Введите силу : "))
            a = float(input("Введите ускорение : "))
            result = F/a
        elif finding == "a":
            F = float(input("Введите силу : "))
            m = float(input("Введите массу : "))
            result = F/m
        else:
            print("Переменная не найдена.")
        print(f"Ответ:{result}")
    case 24:
        result = 0
        print("Сила тяжести.\n Основная формула: N = m * g ")
        finding = str(input("Введите искомое: "))
        if finding == "N":
            g = 9.8
            m = float(input("Введите массу : "))
            result = m * g
        elif finding == "m":
            N = float(input("Введите силу тяжести : "))
            g = 9.8
            result = N / g
        else:
            print("Переменная не найдена.")
        print(f"Ответ:{result}")
    case 25:
        result = 0
        print("Сила трения.\n Основная формула: F = μ * N ")
        finding = str(input("Введите искомое: "))
        if finding == "F":
            mu = float(input("Введите коэффициент трения : "))
            N = float(input("Введите силу тяжести : "))
            result = mu * N
        elif finding == "N":
            mu = float(input("Введите коэффициент трения : "))
            F = float(input("Введите силу тяжести : "))
            result = F / mu
        else:
            print("Переменная не найдена.")
        print(f"Ответ:{result}")
    case 26:
        print("Закон всемирного тяготения.\n Основная формула: F = (G *(m1 * m2))/r^2 ")
        m1 = float(input("Введите массу первого тела : "))
        m2 = float(input("Введите массу второго тела : "))
        r = float(input("Введите расстояние между телами : "))
        G = 6.6743 * (10 ** -11)
        result = (G * (m1 * m2) / r ** 2)
        print(f"Ответ:{result}")
    case 27:
        print("Первая космическая скорость.\n Основная формула: v = √(g * R) ")
        R = float(input("Введите радиус планеты(км) : "))
        g = float(input("Введите ускорение свободного падения планеты : "))
        result = math.sqrt(g * R)
        print(f"Ответ:{result}")
    case 28:
        print("Вторая космическая скорость.\n Основная формула: v = √(2 * g * R) ")
        R = float(input("Введите радиус планеты(км) : "))
        g = float(input("Введите ускорение свободного падения планеты : "))
        result = math.sqrt(2 * g * R)
        print(f"Ответ:{result}")
    case 29:
        result = 0
        print("Вес тела.\n Основная формула: P = m * g ")
        finding = str(input("Введите искомое: "))
        if finding == "P":
            g = 9.8
            m = float(input("Введите массу тела : "))
            result = m * g
        elif finding == "m":
            g = 9.8
            P = float(input("Введите вес тела : "))
            result = P / g
        else:
            print("Переменная не найдена.")
        print(f"Ответ:{result}")
    case 30:
        result = 0
        print("Сила упругости.\n Основная формула: F = k * x ")
        finding = str(input("Введите искомое: "))
        if finding == "F":
            k = float(input("Введите жёсткость : "))
            x = float(input("Введите удлинение (сокращение) предмета : "))
            result = k * x
        elif finding == "k":
            F = float(input("Введите cилу упругости : "))
            x = float(input("Введите удлинение (сокращение) предмета : "))
            result = F / x
        elif finding == "x":
            F = float(input("Введите cилу упругости : "))
            k = float(input("Введите жёсткость : "))
            result = F / k
        else:
            print("Переменная не найдена.")
        print(f"Ответ:{result}")
    case 31:
        print("Внутренняя энергия одноатомного газа.\n Основная формула: U = (3 * m * R * T)/ 2 * M ")
        R = 8.3
        M = float(input("Введите мольную массу : "))
        m = float(input("Введите  массу : "))
        T = float(input("Введите температуру : "))
        result = (3 * m * R * T) / 2 * M
        print(f"Ответ:{result}")
    case 32:
        print("Внутренняя энергия одноатомного газа.\n Основная формула: U = (3 * v * R * T)/ 2  ")
        R = 8.3
        v = float(input("Введите количество вещества : "))
        T = float(input("Введите температуру : "))
        result = (3 * v * R * T) / 2
        print(f"Ответ:{result}")
    case 33:
        print("Удельная теплота газа.\n Основная формула: c = (3 * R * M)/ 2  ")
        R = 8.3
        M = float(input("Введите мольную массу : "))
        result = (3 * R * M) / 2
        print(f"Ответ:{result}")
    case 34:
        result = 0
        print("Работа расширения газа.\n Основная формула: A = ΔV * p ")
        finding = str(input("Введите искомое: "))
        if finding == "A":
            delv = float(input("Введите изменение объёма, если оно не известно введите 0: "))
            if delv == 0:
                v1 = float(input("Введите начальный объём: "))
                v2 = float(input("Введите конечный объём: "))
            p = float(input("Введите давление: "))
            result = delv * p
        elif finding == "p":
            delv = float(input("Введите изменение объёма, если оно не известно введите 0: "))
            if delv == 0:
                v1 = float(input("Введите начальный объём: "))
                v2 = float(input("Введите конечный объём: "))
            A = float(input("Введите работу : "))
            result = A / delv
        else:
            print("Переменная не найдена.")
        print(f"Ответ:{result}")
    case 35:
        print("Работа изотермического расширения газа.\n Основная формула: A = (m * R * T/ M) * ln(v2/v1) ")
        R = 8.3
        M = float(input("Введите мольную массу : "))
        T = float(input("Введите температуру : "))
        m = float(input("Введите  массу : "))
        V1 = float(input("Введите начальный объём : "))
        V2 = float(input("Введите конечный объём : "))
        result = (m * R * T / M) * math.log(V2/V1)
        print(f"Ответ:{result}")
    case 36:
        result = 0
        print("Первый закон термодинамики.\n Основная формула: ΔU = A + Q  ")
        finding = str(input("Введите искомое: "))
        if finding == "ΔU":
            A = float(input("Введите работу : "))
            Q = float(input("Введите количество тепла : "))
            result = A + Q
        elif finding == "A":
            delU = float(input("Введите изменение внутренней энергии, если оно не известно введите 0: "))
            if delU == 0:
                U1 = float(input("Введите начальную внутреннею энергию : "))
                U2 = float(input("Введите конечную внутреннею энергию : "))
            Q = float(input("Введите количество тепла : "))
            result = delU - Q
        elif finding == "Q":
            delU = float(input("Введите изменение внутренней энергии, если оно не известно введите 0: "))
            if delU == 0:
                U1 = float(input("Введите начальную внутреннею энергию : "))
                U2 = float(input("Введите конечную внутреннею энергию : "))
            A = float(input("Введите работу : "))
            result = delU - A
        else:
            print("Переменная не найдена.")
        print(f"Ответ:{result}")
    case 37:
        print("Коэффициент полезного действия теплового двигателя.\n Основная формула: η = An / Q1  ")
        An = float(input("Введите полезную работу : "))
        Q1 = float(input("Введите полученное количество тепла : "))
        result = An / Q1
        print(f"Ответ:{result}")
    case 38:
        print("Коэффициент полезного действия теплового двигателя.\n Основная формула: η = Q1 - Q2 / Q1  ")
        Q2 = float(input("Введите  количество теплоты, отданное холодильнику : "))
        Q1 = float(input("Введите количество теплоты, полученное от нагревателя : "))
        result = Q1 - Q2 / Q1
        print(f"Ответ:{result}")
    case 39:
        print("Максимальная эффективность (КПД) теплового двигателя.\n Основная формула: η = T1 - T2 / T1  ")
        T2 = float(input("Введите температуру холодильника : "))
        T1 = float(input("Введите температуру горячего источника (нагревателя) : "))
        result = T1 - T2 / T1
        print(f"Ответ:{result}")
