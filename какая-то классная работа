if task == 1:
        def rast(x):
            x = x/10
            print( str(x)+ " сантиметры ")
            x = x / 10 
            print( str(x)+ " дцметры ")
            x = x /  10 
            print( str(x)+ " метры ")
            x = x / 1000
            print( str(x)+ " километры ")
            return 
        
        
        x = int(input("Введите расстояние в миллиметрах "))
        rast(x)
        
