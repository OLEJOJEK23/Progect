class Solution:
    def isPalindrome(self, x: int) -> bool:
        k = 0
        x2 = str(x)
        for i in range(len(x2)//2):
            char_1 = x2[i]
            char_2 = x2[len(x2)- 1 - i]
            if char_2 == char_1:
                k += 1
        if k == len(x2)//2:
            return True
        else:
            return False
